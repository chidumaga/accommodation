$('.modal').modal('hide')

if($('#flash').length > 0) {
	$('#flash').remove();
 	$('body').prepend("<%= escape_javascript raw(flash_normal) %>");
 	window.location.reload();
}
else {
 	$('.modal').modal('hide')
 	$('body').prepend("<%= escape_javascript raw(flash_normal) %>");
 	window.location.reload();
}